#!/bin/bash

cd /opt/info
select i in \
    'introductie' \
    'tokeniseren' \
    'parsen' \
    'bekijken en bewerken' \
    'een corpus maken' \
    'een corpus doorzoeken' \
    'afbeeldingen opslaan' \
    'corpora downloaden' \
    'twee versies' \
    'Universal Dependencies' \
    'links'
do
    cat "$i.txt"
    break
done

