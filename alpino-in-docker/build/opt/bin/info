#!/bin/bash

cd /opt/info
select i in \
    'over Alpino in Docker' \
    'starten' \
    'tokeniseren' \
    'parsen' \
    'bekijken en bewerken' \
    'een corpus maken' \
    'een corpus doorzoeken' \
    'corpora downloaden' \
    'twee versies' \
    'Universal Dependencies' \
    'afbeeldingen opslaan' \
    'links'
do
    if [ -f "$i.txt" ]
    then
        cat "$i.txt"
    fi
    break
done

