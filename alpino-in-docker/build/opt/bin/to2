#!/bin/bash

if [ $# != 2 ]
then
    echo
    echo Usage: $0 infile.dact outfile.dact
    echo
    echo This converts infile.dact to outfile.dact
    echo outfile.dact will be for DbXML version 2
    echo
    exit
fi

if [ ! -f "$1" ]
then
    echo File $1 does not exist
    exit
fi

if [ -f "$2" ]
then
    echo File $2 already exists
    exit
fi

/opt/dbxml6/bin/dbxml_dump "$1" | /opt/dbxml2/bin/dbxml_load "$2"

